model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  password String?
  // tenant InteractionRole? @relation(fields: [interactionRoleId], references: [id])
}

model Property {
  id    Int     @id @default(autoincrement())
  name  String?
  address String?
  postalCode String?
